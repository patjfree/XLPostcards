{
  "cli": {
    "version": "16.1.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
"production": {
  "autoIncrement": true,
  "env": {
    "APP_VARIANT": "production"
  },
  "ios": {
    "resourceClass": "m-medium",
    "buildConfiguration": "Release",
    "scheme": "NanaGram",
    "target": "NanaGram"
  },
      "android": {
        "buildType": "app-bundle"
      }
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    }
  }
}
